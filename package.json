{
  "name": "uap-game-engine",
  "version": "1.1.1",
  "description": "Minimalistic 3D game engine for web browsers",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "uap-build": "./bin/uap-build.js"
  },
  "files": [
    "dist/index.js",
    "bin/uap-build.js",
    "README.md"
  ],
  "scripts": {
    "build": "node scripts/check-deps.js && node scripts/build.js",
    "test": "node scripts/test.js",
    "test:demos": "node scripts/test-demos-static.js",
    "test:browser": "node scripts/test-demos-browser.js",
    "test:all": "npm test && npm run test:demos",
    "server": "python3 -m http.server 8080",
    "deps": "node scripts/check-deps.js"
  },
  "keywords": [
    "game-engine",
    "webgl",
    "3d"
  ],
  "author": "mirror12k",
  "repository": {
    "url": "git+https://github.com/mirror12k/uap-game-engine.git"
  },
  "homepage": "https://github.com/mirror12k/uap-game-engine",
  "license": "MIT",
  "dependencies": {
    "esbuild": "^0.19.0",
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "matter-js": "^0.20.0",
    "puppeteer": "^24.31.0"
  }
}
